<tr>
  <td> <%= sale.name %> </td>
  <td> <%= sale.starts_on %> </td>
  <td> <%= sale.ends_on %> </td>
  <td> <%= sale.percent_off %> </td>
  <td> 
    STATUS
    <% if sales.finished? %>
      <span class="label label-success">Finished</span>
    <% elsif sales.upcoming? %>
        <span class="label label-success">Upcoming</span>
    <% else %>
        <span class="label label-success">Active</span>
    <% end %>
  </td>

</tr>